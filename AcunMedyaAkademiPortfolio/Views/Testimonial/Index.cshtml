@Model List<using AcunMedyaAkademiPortfolio.Models.TblTestimonial>
    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/AdminLayout/Index.cshtml";
    }

    <table class="table table-bordered">
        <tr>
            <th>Id</th>
            <th>İsim</th>
            <th>Mesleği</th>
            <th>Güncelle</th>
            <th>Sil</th>
        </tr>
        @foreach(var item in Model)
            {
                <td>@item.TestimonialId</td>
                <td>@item.TestimonialName</td>
                <td>@item.TestimonialTitle</td>
                <td>@item.TestimonialDescription</td>
                <td><a class="btn btn-warning">Güncelle</a></td>
                <td><a class="btn btn-danger">Sil</a></td>
            
            }

    </table>

