
@{
    ViewBag.Title = "CreateSocialMedia";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Create Social Media</h2>

<form method="post">
    <label>Social Media Name</label>
    <input type="text" class="form-control" name="SocialMediaName" />

    <label>Social Media Icon</label>
    <input type="text" class="form-control" name="SocialMediaIcon" />

    <label>Social Media Url</label>
    <input type="text" class="form-control" name="SocialMediaUrl" />

    <br /><br />
    <button class="btn btn-info">Create Social Media </button>
</form>
