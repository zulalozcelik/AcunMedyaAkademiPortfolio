@model AcunMedyaAkademiPortfolio.Models.TblContact
@{
    ViewBag.Title = "UpdateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Update Contact</h2>
<form method="post">
    <label>Contact ID</label>
    <input type="text" class="form-control" name="ContactId" value="@Model.ContactId" />
    <label>Name</label>
    <input type="text" class="form-control" name="Name" value="@Model.Name" />
    <label>Email</label>
    <input type="text" class="form-control" name="Email" value="@Model.Email" />
    <label>Subject</label>
    <input type="text" class="form-control" name="Subject" value="@Model.Subject" />
    <label>Description</label>
    <input type="text" class="form-control" name="Description" value="@Model.Description" />
    <label>CreateDate</label>
    <input type="text" class="form-control" name="CreateDate" value="@Model.CreateDate" />
    <br />
    <button class="btn btn-info">Update Contact</button>
</form>
