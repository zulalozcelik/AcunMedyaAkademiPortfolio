@Model List <using AcunMedyaAkademiPortfolio.Models.TblService>
    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/AdminLayout/Index.cshtml";
    }

    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Title</th>
            <th>Description</th>
            <th>IconUrl</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ServiceId</td>
                <td>@item.Title</td>
                <td>@item.Description</td>
                <td align="center">
                    <img src="@item.IconUrl" style="width:150px" />
                </td>
                <td><a href="/Service/CreateService/@item.ServiceId" class="btn btn-warning">Update</a></td>
                <td><a href="/Service/DeleteService/@item.ServiceId" class="btn btn-danger">Delete</a></td>
            </tr>
        }
    </table>
    <td><a href="/Service/CreateService/" class="btn btn-success">Add new Service</a></td>

