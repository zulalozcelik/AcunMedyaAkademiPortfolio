@Model List <using AcunMedyaAkademiPortfolio.Models.TblService>
    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/AdminLayout/Index.cshtml";
    }

    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Başlık</th>
            <th>Açıklama</th>
            <th>IconUrl</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ServiceId</td>
                <td>@item.Title</td>
                <td>@item.Description</td>
                <td>@item.IconUrl</td>
                <td><a href="/Service/CreateService/@item.ServiceId" class="btn btn-warning">Güncelle</a></td>
                <td><a href="/Service/DeleteService/@item.ServiceId" class="btn btn-danger">Sil</a></td>
            </tr>
        }
    </table>
    <td><a href="/Service/CreateService/" class="btn btn-success">Yeni Servise Ekle</a></td>

