@Model List <using AcunMedyaAkademiPortfolio.Models.TblHobby>
    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/AdminLayout/Index.cshtml";
    }

    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Başlık</th>
            <th>Icon url</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.HobbyId</td>
                <td>@item.Title</td>            
                <td align="center">
                    <img src="@item.IconUrl" style="width:150px" />
                </td>
                <td><a href="/Hobby/CreateHobby/@item.HobbyId" class="btn btn-warning">Güncelle</a></td>
                <td><a href="/Hobby/DeleteHobby/@item.HobbyId" class="btn btn-danger">Sil</a></td>
            </tr>
        }
    </table>
    <td><a href="/Hobby/CreateHobby/" class="btn btn-success">Yeni Hobi Ekle</a></td>


